{
  "name": "webgraph",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/styled-components": "^4.1.8",
    "apollo-boost": "^0.4.3",
    "dotenv": "^8.0.0",
    "flash": "^1.1.0",
    "graphql": "^14.4.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts-ts": "3.1.0",
    "react-stripe-checkout": "^2.6.3",
    "styled-components": "^5.0.0-beta.8"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "schema:download": "apollo schema:download --endpoint=http://localhost:4000/graphql",
    "codegen:generate": "apollo codegen:generate --includes='./src/**/*.tsx'  --localSchemaFile=./schema.json --outputFlat --target=typescript ./src/schemaTypes.ts",
    "gen:types": "npm run schema:download && npm run codegen:generate"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/stripe": "^6.31.8",
    "apollo": "^2.16.1",
    "typescript": "^3.5.3"
  }
}
